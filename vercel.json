{
  "version": 2,
  "outputDirectory": "public",  
  "rewrites": [
    {
      "source": "/api/(.*)",    
      "destination": "/server.js"
    },
    {
      "source": "/(.*)",      
      "destination": "/$1.html"
    },
    {
      "source": "/(.*)",       
      "destination": "/server.js"
    }
  ]
}