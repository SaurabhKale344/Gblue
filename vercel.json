{
  "version": 2,
  "rewrites": [
    {
      "source": "/global.css",
      "destination": "/public/global.css"
    },
    {
      "source": "/style.css",
      "destination": "/public/style.css"
    },
    {
      "source": "/utils.js",
      "destination": "/public/utils.js"
    },
    {
      "source": "/script.js",
      "destination": "/public/script.js"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/public/assets/$1"
    },
    {
      "source": "/(.*\\.(?:png|jpg|jpeg|gif|svg|webp|ico|mp4|webm|ogg|mp3|wav|flac))",
      "destination": "/public/$1"
    },
    {
      "source": "/",
      "destination": "/dist/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/server.js"
    }
  ]
}